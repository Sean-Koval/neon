# Neon Frontend Environment Configuration
# Copy this file to .env.local and fill in your values

# API Configuration
# Base URL for the Neon API server
NEXT_PUBLIC_API_URL=http://localhost:8000

# API Key (optional - can also be set at runtime via UI)
# Format: ae_<environment>_<key>
# Example: ae_live_abc123def456...
# WARNING: Do not commit real API keys to version control
NEXT_PUBLIC_API_KEY=

# Dev Auth Bypass (only works when NODE_ENV=development)
# Set to "true" to bypass JWT/API-key auth with a dev user
AUTH_DEV_BYPASS=true
DEV_WORKSPACE_ID=00000000-0000-0000-0000-000000000001
