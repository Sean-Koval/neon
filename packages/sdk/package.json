{
  "name": "@neon/sdk",
  "version": "0.1.0",
  "description": "Neon Agent Ops SDK - Evals as code",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "neon": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./dist/client/index.js",
      "types": "./dist/client/index.d.ts"
    },
    "./test": {
      "import": "./dist/test/index.js",
      "types": "./dist/test/index.d.ts"
    },
    "./threshold": {
      "import": "./dist/threshold/index.js",
      "types": "./dist/threshold/index.d.ts"
    },
    "./scorers": {
      "import": "./dist/scorers/index.js",
      "types": "./dist/scorers/index.d.ts"
    },
    "./tracing": {
      "import": "./dist/tracing/index.js",
      "types": "./dist/tracing/index.d.ts"
    },
    "./analysis": {
      "import": "./dist/analysis/index.js",
      "types": "./dist/analysis/index.d.ts"
    },
    "./export": {
      "import": "./dist/export/index.js",
      "types": "./dist/export/index.d.ts"
    },
    "./comparison": {
      "import": "./dist/comparison/index.js",
      "types": "./dist/comparison/index.d.ts"
    },
    "./runner": {
      "import": "./dist/runner/index.js",
      "types": "./dist/runner/index.d.ts"
    },
    "./cloud": {
      "import": "./dist/cloud/index.js",
      "types": "./dist/cloud/index.d.ts"
    },
    "./prompts": {
      "import": "./dist/prompts/index.js",
      "types": "./dist/prompts/index.d.ts"
    },
    "./optimization": {
      "import": "./dist/optimization/index.js",
      "types": "./dist/optimization/index.d.ts"
    },
    "./evals": {
      "import": "./dist/evals/index.js",
      "types": "./dist/evals/index.d.ts"
    },
    "./debugging": {
      "import": "./dist/debugging/index.js",
      "types": "./dist/debugging/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@neon/shared": "workspace:*",
    "@neon/llm-providers": "workspace:*",
    "@anthropic-ai/sdk": "^0.32.0",
    "commander": "^12.1.0",
    "glob": "^11.0.0",
    "picocolors": "^1.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "neon",
    "agent",
    "evaluation",
    "llm",
    "testing"
  ]
}
